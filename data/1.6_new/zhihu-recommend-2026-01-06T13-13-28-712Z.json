            "gender": 1,
            "id": "7c5f045f5700d03e93fe53f54eef9d16",
            "url": "https://api.zhihu.com/people/7c5f045f5700d03e93fe53f54eef9d16",
            "user_type": "people",
            "followers_count": 158,
            "is_following": false,
